				"progress": {
					"title": "Progress Bars",
					"description": "The progress element represents task completion. Set value and max for determinate progress; omit value for indeterminate (loading) state. Screen readers announce progress role and percentage automatically. Styleable via ::-webkit-progress-bar and ::-webkit-progress-value pseudo-elements. Falls back to text content in unsupported browsers.",
					"code": "<progress value=\"70\" max=\"100\">",
					"js_title": "JavaScript",
					"js_badge": "JS",
					"js_warning": "Requires: ARIA live regions, role=progressbar, value announcements",
