				"temporal": {
					"title": "Date/Time Inputs",
					"description": "Native date/time inputs (date, time, datetime-local, month, week) provide OS-native pickers with built-in validation, localized formatting, keyboard navigation, and touch support. Attributes min/max constrain range, step controls increments. valueAsDate and valueAsNumber properties enable programmatic access. No external libraries required.",
					"code": "<input type=\"date\"> <input type=\"time\">",
					"js_title": "JavaScript",
					"js_badge": "JS",
					"js_warning": "Requires: Calendar widget, i18n, timezone handling, keyboard navigation, touch events",
					"js_label": "Date (Custom Picker)",
					"js_placeholder": "YYYY-MM-DD",
					"js_picker_content": "[Calendar widget]\nRequires: Date library, i18n,\nkeyboard nav, touch handling,\ntimezone logic.",
					"js_cons_title": "Considerations:",
					"js_cons": [
						"Large date picker libraries",
						"Mobile UX inconsistency",
						"Timezone edge cases"
					],
					"native_title": "Native HTML",
					"native_badge": "HTML",
					"native_note": "Provides: Platform date picker, i18n, validation, keyboard support",
